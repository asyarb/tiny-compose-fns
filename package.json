{
  "name": "tiny-fns",
  "version": "0.10.0",
  "description": "Composable helper functions leveraging modern syntax.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "watch": "yarn build --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'README.md'",
    "prebuild": "rimraf dist",
    "prepublish": "cross-env NODE_ENV=production yarn build",
    "release": "standard-version",
    "test": "jest"
  },
  "homepage": "https://github.com/asyarb/tiny-fns",
  "repository": {
    "type": "git",
    "url": "https://github.com/asyarb/tiny-fns.git"
  },
  "keywords": [
    "tiny",
    "fns",
    "functions"
  ],
  "author": "Anthony Yarbrough",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "husky": "^3.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-terser": "^5.1.1",
    "standard-version": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
