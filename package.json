{
  "name": "tiny-compose-fns",
  "version": "0.4.0",
  "description": "Tiny composeable helper functions.",
  "sideEffects": false,
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'README.md'",
    "prebuild": "rimraf dist",
    "prepublish": "yarn build",
    "release": "standard-version",
    "test": "jest",
    "benchmark": "cd benchmark && yarn benchmark"
  },
  "homepage": "https://github.com/asyarb/tiny-compose-fns",
  "repository": {
    "type": "git",
    "url": "https://github.com/asyarb/tiny-compose-fns.git"
  },
  "keywords": [
    "tiny",
    "fns",
    "functions",
    "compose",
    "fp",
    "functional",
    "programming"
  ],
  "author": "Anthony Yarbrough",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "benchmark": "^2.1.4",
    "compose-tiny": "^1.1.3",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-terser": "^5.1.1",
    "standard-version": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "curriable": "^1.3.0"
  }
}
