{
  "name": "tiny-compose-fns",
  "version": "0.3.0",
  "description": "Tiny composeable helper functions.",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "microbundle --compress",
    "dev": "microbundle watch",
    "format": "prettier --write 'src/**/*.js' 'README.md'",
    "prebuild": "rimraf dist",
    "prepublish": "yarn build",
    "release": "standard-version",
    "test": "jest"
  },
  "homepage": "https://github.com/asyarb/tiny-fns",
  "repository": {
    "type": "git",
    "url": "https://github.com/asyarb/tiny-fns.git"
  },
  "keywords": [
    "tiny",
    "fns",
    "functions"
  ],
  "author": "Anthony Yarbrough",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-eslint": "^10.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "husky": "^3.0.1",
    "microbundle": "^0.11.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "standard-version": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
